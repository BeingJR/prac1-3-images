curl -SL https://github.com/docker/compose/releases/download/v2.24.6/dockercomposelinux-x86_64 -o /usr/local/bin/docker-compose
chmod +x /user/local/bin/docker-compose
docker compose version 
mkdir composetest
cd composetest 
nano app.py
nano requirements.txt
nano Dockerfile
nano compose.yml
docker compose up 
